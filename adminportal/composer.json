{
    "name": "example/adminportal",
    "description": "Admin Portal on Laravel 12 with roles, activity log, backups, social login, 2FA, Datatables, AJAX.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^12.0",
        "laravel/socialite": "^5.12",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-backup": "^8.7",
        "yajra/laravel-datatables-oracle": "^11.0",
        "intervention/image": "^3.8",
        "anhskohbo/no-captcha": "^3.5",
        "pragmarx/google2fa-laravel": "^2.4",
        "lab404/laravel-impersonate": "^1.11"
    },
    "require-dev": {
        "laravel/breeze": "^2.2",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi || true"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}